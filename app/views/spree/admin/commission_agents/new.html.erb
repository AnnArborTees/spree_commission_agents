<% content_for :page_title do %>
  <%= Spree.t(:new_commission_agent) %>
<% end %>

<%= form_for [:admin, @product, @commission_agent], html: { multipart: true } do |f| %>
   <fieldset data-hook="new_image">
    
    <%= render partial: 'form', locals: { f: f } %>
    <div class="form-actions" data-hook="buttons">
      <%= button Spree.t('actions.update'), 'save' %>
      <span class="or"><%= Spree.t(:or) %></span>
      <%= button_link_to Spree.t('actions.cancel'), admin_product_commission_agents_url(@product), icon: 'delete', id: 'cancel_link' %>
    </div>
  </fieldset>
<% end %>
